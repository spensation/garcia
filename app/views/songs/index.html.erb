<!-- views/songs/index.html -->
<div>
	<% if logged_in? %>
	<h4> Welcome, <%=link_to current_user.username, user_path(current_user)%></h4>
	
	<h1>All The Songs!</h1>
	

	<div>
		<% @songs.each do |song| %>
			<p><%= link_to song.title, song_path(song) %></p>
		<% end %>
	</div>
	<% else %>
		<h1>All The Songs!</h1>
	

		<div>
			<% @songs.each do |song| %>
				<p><%= link_to song.title, song_path(song) %></p>
			<% end %>
		</div>
	<% end %>
</div>